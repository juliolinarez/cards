#!/usr/bin/env ruby

# This script starts the Sidekiq worker process
# Usage: bin/jobs

require "pathname"
ENV["BUNDLE_GEMFILE"] ||= File.expand_path("../../Gemfile",
  Pathname.new(__FILE__).realpath)

require "rubygems"
require "bundler/setup"

# Start Sidekiq
exec "bundle", "exec", "sidekiq", "-C", "config/sidekiq.yml"
